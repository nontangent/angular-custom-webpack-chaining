{
	"$schema": "http://json-schema.org/schema",
	"$id": "custom-webpack-ng-add",
	"title": "Angular Custom Webpack ng-add schematic",
	"type": "object",
	"properties": {
		"project": {
			"type": "string",
			"description": "Name of the project.",
			"$default": {
				"$source": "projectName"
			}
		},
		"path": {
			"type": "string",
			"description": "Path of the custom webpack file.",
			"default": "./extra-webpack.config.js"
		}
	}
}
